<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="142.0" prefWidth="314.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ihm_project.tictactoe.EndGameController">
   <children>
      <HBox layoutY="103.0" prefHeight="25.0" prefWidth="314.0" spacing="25.0">
         <children>
            <Button fx:id="exitButton" mnemonicParsing="false" onAction="#onExitButtonClicked" text="Quitter" />
            <Button fx:id="retryButton" mnemonicParsing="false" onAction="#onRetryButtonClicked" text="Recommencer" />
            <Button fx:id="newGameButton" mnemonicParsing="false" onAction="#onNewGameButtonClicked" text="Nouvelle Partie" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </HBox>
      <Label fx:id="gameResultLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="19.0" layoutY="38.0" prefHeight="36.0" prefWidth="277.0" text="Label" AnchorPane.bottomAnchor="74.0" AnchorPane.topAnchor="38.0">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
